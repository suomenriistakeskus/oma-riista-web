{
    "globalstrict": true,
    "browser": true,
    "node": false,
    "esnext": false,
    "forin": true,
    "futurehostile": true,
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "jquery": true,
    "latedef": "nofunc",
    "laxbreak": true,
    "loopfunc": true,
    "maxlen": 200,
    "maxdepth": 3,
    "maxparams": 30,
    "maxstatements": 100,
    "noarg": true,
    "nonbsp": true,
    "undef": true,
    "unused": false,
    "trailing": true,
    "smarttabs": true,
    "white": true,
    "validthis": true,
    "globals": {
        "angular": false,
        "console": false,
        "_": false,
        "L": false,
        "moment": false,
        "greinerHormann": false,
        "QRCode": false,
        // Angular Mocks
        "inject": false,
        // JASMINE
        "describe": false,
        "it": false,
        "before": false,
        "beforeEach": false,
        "after": false,
        "afterEach": false,
        "expect": false
    }
}
