<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--
    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/
-->
<urlrewrite>
    <rule match-type="regex">
        <from>^/v/[0-9A-Za-z_.\-]+/(.*)$</from>
        <to>/frontend/$1</to>
    </rule>

    <rule match-type="regex">
        <from>^/privacy_policy$</from>
        <to type="redirect">/#/privacy_policy</to>
    </rule>

    <rule match-type="regex">
        <from>^/privacy_policy/sv$</from>
        <to type="redirect">/#/privacy_policy?lang=sv</to>
    </rule>

    <rule match-type="regex">
        <from>^/privacy_policy/en$</from>
        <to type="redirect">/#/privacy_policy?lang=en</to>
    </rule>
</urlrewrite>
